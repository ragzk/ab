
<!DOCTYPE html>
<html lang="en" ng-app="APP">
	<head>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
		<!-- Latest compiled and minified CSS -->
		<link href="/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
		<link rel="shortcut icon" href="/public/images/favicon.png">
		<link href="http://cdnjs.cloudflare.com/ajax/libs/select2/3.4.8/select2.css" rel="stylesheet" />
		<link href="http://cdnjs.cloudflare.com/ajax/libs/select2/3.4.8/select2-bootstrap.css" />
		<link href='http://fonts.googleapis.com/css?family=Lato:400,700' rel='stylesheet' type='text/css'>
		<link rel="stylesheet" href="/stylesheets/fontawesome.css" type="text/css" media="screen" charset="utf-8">
		<title>@model.title</title>
		<link rel="stylesheet" href="/stylesheets/custom.min.css" type="text/css" media="screen" charset="utf-8">
		<link href="/bower_components/slick-carousel/slick/slick.css" rel="stylesheet" />
		<link href="/bower_components/slick-carousel/slick/slick-theme.css" rel="stylesheet" />
		<link href="/bower_components/bxslider-4/dist/jquery.bxslider.min.css" rel="stylesheet" />
	</head>
	<body>		
		<div id="main-container" class="container" ng-controller="mainController">
			<div id="header-container">
				<div ng-include src="'Header.html'"></div>
			</div>
			<div id="body-container">
		<div class="sidebar">
			<div id="MainMenu" class="sideMenu" ng-controller="sidebarController">
				<div id="container">

					<div id="nav" class="list-group panel">   
						<div class="title parent"><a class="list-group-item  route" href="/buy" ng-click="toggleMenu($event)">Buy<i ng-show="toggle" class="fa fa-caret-down"></i><i ng-hide="toggle" class="fa fa-caret-up"></i></a>
							<div>
									<a class="list-group-item child route" href="/buy/residential">Residential</a>
									<a class="list-group-item child route" href="/buy/inspection">Open for Inspection</a>
									<a class="list-group-item child route" href="/buy/Auction">Auction</a>
							</div>
						</div>
						<div class="title parent"><a class="list-group-item parent route" href="/rent" ng-click="toggleMenu($event)">Rent<i ng-show="toggle" class="fa fa-caret-down"></i><i ng-hide="toggle" class="fa fa-caret-up"></i></a>
							<div>
									<a class="list-group-item child route" href="/rent/residential">Residential</a>
									<a class="list-group-item child route" href="/rent/inspection">Open for Inspection</a>
									<a class="list-group-item child route" href="/rent/Auction">Auction</a>
							</div>
						</div>
						<div class="title list-group-item parent route"><a href="/sold">Sold</a>
						</div>
					</div>    
				</div>
			</div>
		</div>
				@html.block('content')
			</div>
			<footer>
				<div>
					<div ng-include src="'Footer.html'"></div>
				</div>
			</footer>
		</div>
	</body>
